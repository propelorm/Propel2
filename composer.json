{
    "name": "propel/propel",
    "type": "library",
    "description": "Propel2 is an open-source Object-Relational Mapping (ORM) for PHP 5.3",
    "keywords": [
        "ORM",
        "persistence",
        "Active Record"
    ],
    "homepage": "http://www.propelorm.org/",
    "license": "MIT",
    "authors": [
        {
            "name": "William Durand",
            "email": "william.durand1@gmail.com"
        }
    ],
    "repositories": {
        "phing": {
            "pear": {
                "url": "http://pear.phing.info"
            }
        }
    },
    "require": {
        "php": ">=5.3.2",
        "symfony/yaml": ">=2.0",
        "symfony/console": ">=2.0",
        "monolog/monolog": ">=1.0.2",
        "symfony/finder": ">=2.0",
        "phing": "2.4.9"
    },
    "autoload": {
        "psr-0": {
            "Propel": "src/"
        }
    },
    "bin": [
        "bin/propel"
    ]
}
